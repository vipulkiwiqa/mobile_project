<?xml version="1.0" encoding="UTF-8"?>
<suite name="Red Bus Mobile Application" verbose="10" parallel="tests"
	thread-count="1">

	<!-- Put IP of localhost/Grid Hub machine -->
	<parameter name="selenium.host" value="localhost" />

	<!-- Put Port of localhost RC/Grid Hub machine -->
	<parameter name="selenium.port" value="4444" />

	<!-- Put target browser name as below: 1) Firefox : firefox 2) chrome : 
		chrome 3) IExplorer : ie8 / ie9 -->
	<parameter name="selenium.browser" value="chrome" />

	<!-- <parameter name="selenium.url" value="http://dashboard.staging.clearfunds.io/login" 
		/> -->

	<!-- <parameter name="selenium.url" value="http://demoqa.com/registration/" 
		/> -->

	<test name="create note:: 01">
		<classes>
			<class name="android.workpulsetask.index.RedBusIndex">
				<methods>
					<include name="SignupwithRedBus" />
				</methods>
			</class>
		</classes>
	</test>

</suite>